{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "ERP system for ipsmold",
  "dependencies": {},
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "SET BROWSER=none&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "foreman": "^3.0.1",
    "typescript": "^3.8.3",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "date-fns": "^2.11.0",
    "history": "^4.10.1",
    "material-table": "^1.57.2",
    "material-ui-search-bar": "^1.0.0-beta.14",
    "notistack": "^0.9.9",
    "react": "^16.13.1",
    "react-barcode": "^1.4.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-to-print": "^2.6.3",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "xlsx": "^0.15.6"
  },
  "author": {
    "name": "wudaown",
    "email": "wudaown@gmail.com",
    "url": "https://github.com/wudaown"
  },
  "build": {
    "appId": "com.ipsmold.ErP",
    "productName": "ErP",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "assets/win.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "publisherName": "${author}"
    },
    "files": [
      "src/electron-starter.js",
      "build/**/*",
      "node_modules/**/*",
      "assets/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraMetadata": {
      "main": "src/electron-starter.js"
    }
  }
}
